{
    "name": "thoughtco/statamic-cache-tracker",
    "autoload": {
        "psr-4": {
            "Thoughtco\\StatamicCacheTracker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thoughtco\\StatamicCacheTracker\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.3",
        "pixelfear/composer-dist-plugin": "^0.1.6",
        "statamic/cms": "^6.0"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^8.0",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^4.0",
        "phpunit/phpunit": "^12.0"
    },
    "extra": {
        "download-dist": [
            {
                "url": "https://github.com/thoughtco/statamic-cache-tracker/releases/download/{$version}/dist.tar.gz",
                "path": "dist"
            }
        ],
        "statamic": {
            "name": "Statamic Cache Tracker",
            "description": "Keep a record of what items are used on each page and invalidate those pages when they are saved."
        },
        "laravel": {
            "providers": [
                "Thoughtco\\StatamicCacheTracker\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "pixelfear/composer-dist-plugin": true
        }
    },
    "minimum-stability": "alpha"
}
